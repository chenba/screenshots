  },
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:flake8": "pushd .. && make flake8 && popd",
    "lint:js": "eslint .",
    "lint:sass": "pushd .. && sass-lint -v -q && popd",
    "postlint": "nsp check -o summary",
    "nsp": "nsp check",
    "posttest": "npm run lint",
    "test": "pushd .. && mocha test/server/unit && popd",
    "test:server": "pushd .. && make .venv && cd test/server && ../../.venv/bin/pytest -m 'not slow' && popd",
    "make_versions_exact": "node ../bin/build-scripts/make_versions_exact.js",
    "update_outdated": "david update && npm run make_versions_exact && npm update && npm outdated",
    "fill_database": "pushd .. && make .venv && ./.venv/bin/python  ./bin/load_test_exercise.py -q --little-image --create=50 --read-shot=0 --read-my-shots=0 --search=0 --new-account http://localhost:10080 && popd",
    "svgo": "svgo --folder=../static/img"
  }
}
